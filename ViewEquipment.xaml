<UserControl x:Class="NETD3202_RyanClayson_Lab2.ViewEquipment"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:NETD3202_RyanClayson_Lab2"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
  <!--Beginning of  grid-->
    <Grid>
        <StackPanel Grid.Column="1" Margin="50" Orientation="Vertical">
          <Label Content="View Equipment Lent Out" FontSize="25" HorizontalAlignment="Center" />
          <!--Start of data grid-->
          <DataGrid Name="equipmentGrid"/>
        </StackPanel>
      <!--End of stack panel-->
    </Grid>
  <!--End of Grid-->
</UserControl>
<!--End of User Control-->
